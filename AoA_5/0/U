FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Velocidad para Ã¡ngulo de ataque: 0 grados
// Ux = 10.000000 m/s, Uy = 0 m/s

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (17.931492 1.568790 0);

boundaryField
{
    inlet
{
        type fixedValue;
        value $internalField   uniform (17.931492 1.568790 0);
}


    outlet
    {
         type zeroGradient;
    }

    airfoilInternal
    {
        type noSlip;
    }

    airfoilExternal
    {
        type noSlip;
    }

    externalWalls
{
        type fixedValue;
        value $internalField   uniform (17.931492 1.568790 0);
}


    frontAndBack
    {
          type fixedValue;
        value $internalField   uniform (17.931492 1.568790 0);
    }

    avion
    {
        type noSlip;
    }
}

