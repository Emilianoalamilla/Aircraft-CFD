/*-----------Angle of Attack: 0 degrees----------- */

//c 1; //tamaño de cuerda
//e_max 0.012; //espesor máximo

canard_1
{
    type            forceCoeffs;
    libs            ("libforces.so");

    writeControl    timeStep;
    timeInterval    1;

    log             yes;

    patches         ("avion");

    rho             rhoInf;
    rhoInf          0.900172;

        liftDir         (-.087155 .996194 0);
        dragDir         (.996194 .087155 0);
    pitchAxis       (0 0 -1);    //znegativo

    lRef            0.61;

    Aref            2.6680;

    CofR            (0 0 0);

    magUInf         18;
}

